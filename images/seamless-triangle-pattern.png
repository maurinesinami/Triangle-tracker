       function maxareavalidation()
		{
			var	sele_id = $("#size_format").val();
            var s_width = $("#txtwidth").val();
            var s_height = $("#txtheight").val();
			var maxarea=$("#maxarea").val();
			///var maxarea=20;
            var width, height, area;
            if(sele_id != "" && s_width > 0 && s_height > 0) 
			{ 
                switch(sele_id) {
                    case 'cm':  
                        width = (s_width)*(1/30.48);
                        height  = (s_height)* (1/30.48);
                        area = width * height;
                    break;
                    case 'inch':  
                        width = (s_width)*(1/12);
                        height  = (s_height)* (1/12);
                        area = width * height ;
                    break;
                    case 'feet':  
                        width = s_width;
                        height  = s_height;
                        area = width * height ;
                    break;
                }
								
                if(area < maxarea) { $("#areaerror").show();$("#areaerror").text("The min order limit is "+maxarea+" Sq.Ft.");return false; } 
				else {  $("#areaerror").hide(); return true;}
            } 
			else 
			{
                var sele_id = $("#prop_size").attr("value");
                if(sele_id != "") 
				{ 
					return true; 
				} 
				else 
				{ 
					return false; 
				}
            }	
		}
        

